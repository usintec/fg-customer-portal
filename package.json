{
  "name": "fg-customer-portal",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn w:css\" \"vite\"",
    "build": "yarn build:css && run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "build:css": "cross-env NODE_ENV=production tailwindcss -c ./tailwind.print.config.js -o ./dist/print.css -m",
    "w:css": "tailwindcss -c ./tailwind.print.config.js -o ./public/print.css -w"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "dateformat": "^5.0.3",
    "flag-icons": "^6.11.0",
    "paperizer": "^0.1.6",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-toastification": "^2.0.0-rc.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/dateformat": "^5.0.0",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.16.1",
    "flag-icon-css": "^4.1.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-mkcert": "^1.16.0",
    "vue-tsc": "^1.8.8"
  }
}
